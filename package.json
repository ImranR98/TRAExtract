{
  "name": "tra-extract",
  "version": "1.0.0",
  "description": "Extract TRA receipt information from a list of URLs",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "electron-forge make --platform darwin",
    "make-deb": "electron-forge make --platform linux --targets @electron-forge/maker-deb",
    "make-win": "electron-forge make --platform win32",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Imran Remtulla",
    "email": "iremtulla15@gmail.com"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icon/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "TRAExtract",
            "title": "TRAExtract",
            "setupIcon": "./icon/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ],
          "config": {
            "productName": "TRAExtract"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "./icon/icon.png",
            "productName": "TRAExtract"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "./icon/icon.png",
            "productName": "TRAExtract"
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "12.0.2"
  }
}